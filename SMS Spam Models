    "input_size = X_train.shape[1]\n",
    "hidden_size = 30\n",
    "output_size = 1 \n",
    "learning_rate = 1\n",
    "epochs = 1000"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "be9590af",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-12-01T23:08:36.779647Z",
     "iopub.status.busy": "2024-12-01T23:08:36.779401Z",
     "iopub.status.idle": "2024-12-01T23:08:36.786891Z",

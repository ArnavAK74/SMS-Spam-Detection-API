    "tags": []
   },
   "outputs": [],
   "source": [
    "W1 = np.random.randn(input_size, hidden_size) * np.sqrt(2. / input_size)\n",
    "b1 = np.zeros((1, hidden_size))\n",
    "W2 = np.random.randn(hidden_size, output_size) * np.sqrt(2. / hidden_size)\n",
    "b2 = np.zeros((1, output_size))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "ccf0e2e0",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-12-01T23:08:36.812552Z",
     "iopub.status.busy": "2024-12-01T23:08:36.812307Z",
     "iopub.status.idle": "2024-12-01T23:09:14.434355Z",
     "shell.execute_reply": "2024-12-01T23:09:14.433145Z"
    },
    "papermill": {
     "duration": 37.638168,
     "end_time": "2024-12-01T23:09:14.438100",
     "exception": false,
     "start_time": "2024-12-01T23:08:36.799932",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Epoch 0/1000 - Loss: 0.6922000286224902\n",
      "Epoch 100/1000 - Loss: 0.2191449869590741\n",
      "Epoch 200/1000 - Loss: 0.07683868175321287\n",
      "Epoch 300/1000 - Loss: 0.04514818678488805\n",
      "Epoch 400/1000 - Loss: 0.03015401884013278\n",
      "Epoch 500/1000 - Loss: 0.021496714830928965\n",
      "Epoch 600/1000 - Loss: 0.016172402227029883\n",
      "Epoch 700/1000 - Loss: 0.012724522516231728\n",
      "Epoch 800/1000 - Loss: 0.01035874027465173\n",
      "Epoch 900/1000 - Loss: 0.008653016812649095\n"
     ]
    }
   ],
   "source": [
    "for epoch in range(epochs):\n",
    "    \n",
    "    Z1 = np.dot(X_train, W1) + b1\n",
    "    A1 = leaky_relu(Z1)\n",
    "    Z2 = np.dot(A1, W2) + b2\n",
    "    A2 = sigmoid(Z2)\n",
    "    \n",
    "    y_train_np = y_train.values.reshape(-1, 1)\n",
    "\n",
    "    m = y_train.shape[0]\n",
    "    loss = -(1/m) * np.sum(y_train_np * np.log(A2 + 1e-8) + (1 - y_train_np) * np.log(1 - A2 + 1e-8))\n",
    "    \n",
    "    dZ2 = A2 - y_train_np.reshape(-1, 1)\n",
    "    dW2 = (1/m) * np.dot(A1.T, dZ2)\n",
    "    db2 = (1/m) * np.sum(dZ2, axis=0, keepdims=True)\n",
    "\n",
    "    dA1 = np.dot(dZ2, W2.T)\n",
    "    dZ1 = dA1 * leaky_relu_derivative(A1)\n",
    "    dW1 = (1/m) * np.dot(X_train.T, dZ1)\n",
    "    db1 = (1/m) * np.sum(dZ1, axis=0, keepdims=True)\n",
    "\n",
    "    W1 -= learning_rate * dW1\n",
    "    b1 -= learning_rate * db1\n",
    "    W2 -= learning_rate * dW2\n",
    "    b2 -= learning_rate * db2\n",
    "\n",
    "    if epoch % 100 == 0:\n",
    "        print(f'Epoch {epoch}/{epochs} - Loss: {loss}')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "0d7e1bbc",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2024-12-01T23:09:14.513730Z",
     "iopub.status.busy": "2024-12-01T23:09:14.513221Z",
